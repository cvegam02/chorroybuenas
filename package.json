{
  "name": "loteria-personalizada",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "deploy:functions": "dotenv -e .env -- npx supabase functions deploy create-payment-preference",
    "deploy:functions:all": "npx supabase functions deploy create-payment-preference && npx supabase functions deploy webhook-mercadopago --no-verify-jwt && npx supabase functions deploy credit-payment-on-return && npx supabase functions deploy transform-loteria",
    "build:prod": "dotenv -e .env.production -- vite build",
    "db:push": "npx supabase db push",
    "db:migrate": "npx supabase db push"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.94.0",
    "i18next": "^25.8.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "pdf-lib": "^1.17.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^16.5.4",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.11.0",
    "recharts": "^3.7.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/react-router-dom": "^5.3.3",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "supabase": "^2.76.5",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
